<p id="notice"><%= notice %></p>

<h1>Example Resources</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @example_resources.each do |example_resource| %>
      <tr>
        <td><%= example_resource.name %></td>
        <td><%= link_to 'Show', example_resource %></td>
        <td><%= link_to 'Edit', edit_example_resource_path(example_resource) %></td>
        <td><%= link_to 'Destroy', example_resource, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Example Resource', new_example_resource_path %>

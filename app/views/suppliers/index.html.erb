<h1>Suppliers</h1>
<table class='table table-hover table-sm'>
	<thead>
		<th>#</th>
		<th>Name</th>
		<th> </th>
	</thead>
	<tbody>
		<% @suppliers.each do |supplier| %>
		<tr>
			<td><strong><%= supplier.id%></strong></td>
			<td><%= supplier.name %></td>
			<td></td>
			<td></td>			
			<td><%= link_to("show", supplier)
				%>
		</tr>
		<% end %>
	</tbody>
</table>


<%= link_to 'New Supplier', new_supplier_path %>

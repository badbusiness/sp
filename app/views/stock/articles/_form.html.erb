
<%= simple_form_for @stock_article do |f| %>
  <%= f.input :number %>
  <%= f.input :description %>   
  <%= f.input :specification %>
  <%= f.association :article_group%>
  <%= f.input :purchase_price %>
  <%= f.input :unit %>
  Btw percentage: <%= f.association :vat, label_method: :name_for_select%>
  <%= f.button :submit %>
  
<% end %>

